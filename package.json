{
  "name": "@willsoto/vue-remark",
  "license": "Apache-2.0",
  "version": "0.5.0",
  "main": "./dist/VueRemark.umd.js",
  "module": "./dist/VueRemark.esm.js",
  "files": [
    "dist",
    "lib",
    "typings"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "prerelease": "yarn build && yarn typings",
    "release": "standard-version --sign",
    "postrelease": "git push --follow-tags origin master",
    "serve": "vue-cli-service serve",
    "start": "yarn serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/vue-remark.d.ts",
  "dependencies": {
    "lodash.flatmap": "~4.5.0",
    "lodash.get": "~4.4.2",
    "remark-parse": "~7.0.1",
    "unified": "~8.4.1",
    "vue-class-component": "~7.1.0"
  },
  "peerDependencies": {
    "vue": ">=2.5.22"
  },
  "devDependencies": {
    "@types/jest": "~24.0.22",
    "@types/lodash.flatmap": "~4.5.6",
    "@types/lodash.get": "~4.4.6",
    "@typescript-eslint/parser": "~2.6.1",
    "@vue/cli-plugin-e2e-cypress": "~4.0.5",
    "@vue/cli-plugin-eslint": "~4.0.5",
    "@vue/cli-plugin-typescript": "~4.0.5",
    "@vue/cli-plugin-unit-jest": "~4.0.5",
    "@vue/cli-service": "~4.0.5",
    "@vue/eslint-config-prettier": "~5.0.0",
    "@vue/eslint-config-typescript": "~4.0.0",
    "@vue/test-utils": "~1.0.0-beta.29",
    "babel-eslint": "~10.0.3",
    "eslint": "~6.6.0",
    "eslint-plugin-prettier": "~3.1.1",
    "eslint-plugin-vue": "~6.0.0",
    "lint-staged": "~9.4.2",
    "prettier": "~1.18.2",
    "prettier-package-json": "~2.1.0",
    "raw-loader": "~3.1.0",
    "remark-shortcodes": "~0.2.1",
    "rimraf": "~3.0.0",
    "rollup": "~1.26.5",
    "rollup-plugin-commonjs": "~10.1.0",
    "rollup-plugin-filesize": "~6.2.1",
    "rollup-plugin-typescript": "~1.0.1",
    "rollup-plugin-vue": "~5.1.2",
    "standard-version": "~7.0.0",
    "ts-jest": "~24.1.0",
    "tslib": "~1.10.0",
    "typescript": "~3.7.2",
    "vue": "~2.6.8",
    "vue-template-compiler": "~2.6.10"
  },
  "browserslist": [
    "> 20%"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "root": true,
    "rules": {}
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{js,vue,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "volta": {
    "node": "12.13.0",
    "yarn": "1.19.1"
  }
}
