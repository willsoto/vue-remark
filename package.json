{
  "browserslist": [
    "> 20%"
  ],
  "dependencies": {
    "lodash.flatmap": "~4.5.0",
    "lodash.get": "~4.4.2",
    "remark-parse": "~7.0.1",
    "unified": "~7.1.0",
    "vue-class-component": "~7.1.0"
  },
  "devDependencies": {
    "@types/jest": "~24.0.18",
    "@types/lodash.flatmap": "~4.5.6",
    "@types/lodash.get": "~4.4.6",
    "@typescript-eslint/parser": "~2.0.0",
    "@vue/cli-plugin-e2e-cypress": "~3.11.0",
    "@vue/cli-plugin-eslint": "~3.11.0",
    "@vue/cli-plugin-typescript": "~3.11.0",
    "@vue/cli-plugin-unit-jest": "~3.10.0",
    "@vue/cli-service": "~3.10.0",
    "@vue/eslint-config-prettier": "~5.0.0",
    "@vue/eslint-config-typescript": "~4.0.0",
    "@vue/test-utils": "~1.0.0-beta.29",
    "babel-eslint": "~10.0.2",
    "eslint": "~5.16.0",
    "eslint-plugin-prettier": "~3.1.0",
    "eslint-plugin-vue": "~5.2.3",
    "lint-staged": "~9.2.3",
    "prettier": "~1.18.2",
    "raw-loader": "~3.1.0",
    "remark-shortcodes": "~0.2.1",
    "rimraf": "~3.0.0",
    "rollup": "~1.20.0",
    "rollup-plugin-commonjs": "~10.0.2",
    "rollup-plugin-filesize": "~6.2.0",
    "rollup-plugin-typescript": "~1.0.1",
    "rollup-plugin-vue": "~5.0.1",
    "standard-version": "~7.0.0",
    "ts-jest": "~24.0.2",
    "tslib": "~1.10.0",
    "typescript": "~3.5.3",
    "vue": "~2.6.8",
    "vue-template-compiler": "~2.6.10"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "root": true,
    "rules": {}
  },
  "files": [
    "lib",
    "dist",
    "typings"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "main": "./dist/VueRemark.umd.js",
  "module": "./dist/VueRemark.esm.js",
  "name": "@willsoto/vue-remark",
  "peerDependencies": {
    "vue": ">=2.5.22"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "postrelease": "git push --follow-tags origin master",
    "prebuild": "yarn lint && rimraf dist",
    "prerelease": "yarn build && yarn typings",
    "release": "standard-version --sign",
    "serve": "vue-cli-service serve",
    "start": "yarn serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/vue-remark.d.ts",
  "version": "0.5.0",
  "volta": {
    "node": "10.15.3",
    "yarn": "1.16.0"
  }
}