{
  "name": "@willsoto/vue-remark",
  "license": "Apache-2.0",
  "version": "0.5.0",
  "main": "./dist/VueRemark.umd.js",
  "module": "./dist/VueRemark.esm.js",
  "files": [
    "dist",
    "lib",
    "typings"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "prerelease": "yarn build && yarn typings",
    "release": "standard-version --sign",
    "postrelease": "git push --follow-tags origin master",
    "serve": "vue-cli-service serve",
    "start": "yarn serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/vue-remark.d.ts",
  "dependencies": {
    "lodash.flatmap": "4.5.0",
    "lodash.get": "4.4.2",
    "remark-parse": "8.0.2",
    "unified": "9.0.0",
    "vue-class-component": "7.2.3"
  },
  "peerDependencies": {
    "vue": ">=2.5.22"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/lodash.flatmap": "4.5.6",
    "@types/lodash.get": "4.4.6",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@vue/cli-plugin-e2e-cypress": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-plugin-typescript": "4.3.1",
    "@vue/cli-plugin-unit-jest": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "5.0.2",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-vue": "6.2.2",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "raw-loader": "4.0.1",
    "remark-shortcodes": "0.3.1",
    "rimraf": "3.0.2",
    "rollup": "2.10.4",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "9.0.0",
    "rollup-plugin-typescript": "1.0.1",
    "rollup-plugin-vue": "5.1.7",
    "standard-version": "8.0.0",
    "ts-jest": "26.0.0",
    "tslib": "2.0.0",
    "typescript": "3.9.3",
    "vue": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "browserslist": [
    "> 20%"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "root": true,
    "rules": {}
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{js,vue,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "volta": {
    "node": "12.13.0",
    "yarn": "1.19.1"
  }
}
