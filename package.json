{
  "browserslist": [
    "> 20%"
  ],
  "dependencies": {
    "lodash.flatmap": "4.5.0",
    "lodash.get": "4.4.2",
    "vue-class-component": "6.3.2"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/lodash.flatmap": "4.5.4",
    "@types/lodash.get": "4.4.4",
    "@typescript-eslint/parser": "1.1.0",
    "@vue/cli-plugin-e2e-cypress": "3.3.0",
    "@vue/cli-plugin-eslint": "3.3.0",
    "@vue/cli-plugin-typescript": "3.3.0",
    "@vue/cli-plugin-unit-jest": "3.3.0",
    "@vue/cli-service": "3.3.1",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/eslint-config-typescript": "3.2.1",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-eslint": "10.0.1",
    "eslint": "5.12.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "5.1.0",
    "lint-staged": "8.1.1",
    "prettier": "1.16.1",
    "raw-loader": "1.0.0",
    "remark-parse": "~6.0.3",
    "rimraf": "2.6.3",
    "rollup": "1.1.2",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-typescript": "1.0.0",
    "rollup-plugin-vue": "4.6.1",
    "standard-version": "4.4.0",
    "ts-jest": "23.10.5",
    "tslib": "1.9.3",
    "typescript": "3.2.4",
    "unified": "7.1.0",
    "vue": "2.6.0-beta.2",
    "vue-template-compiler": "2.6.0-beta.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "root": true,
    "rules": {}
  },
  "files": [
    "lib",
    "dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/lib/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "name": "@willsoto/vue-remark",
  "peerDependencies": {
    "remark-parse": "~6.0.3",
    "unified": "~7.1.0",
    "vue": ">=2.5.22"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "lint": "vue-cli-service lint",
    "postrelease": "git push --follow-tags origin master",
    "prebuild": "yarn lint && rimraf dist",
    "prerelease": "yarn build && yarn typings",
    "release": "standard-version --sign",
    "serve": "vue-cli-service serve",
    "start": "yarn serve",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/vue-remark.d.ts",
  "version": "0.3.0"
}
